# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines
[*]
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

[*.java]
# tab indentation
indent_style = tab
charset = utf-8

[*.cs]
# 4 space indentation
indent_style = space
indent_size = 4